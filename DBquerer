package friday;
import java.util.Scanner;
public class DBquerer 
{
	public static void main(String[] args) {
		System.out.println("Hey,this is friday.How can I help you?");
		Scanner user_input_scanner = new Scanner(System.in);
		for(int i=0;i>=0;i++)
		{
			String s = user_input_scanner.next();
			String result = DBconn.query(s);
			if(result=="dpe")
			{
				i=-1;
			}
			System.out.println(result);
			result=" ";
		}
	}
}
